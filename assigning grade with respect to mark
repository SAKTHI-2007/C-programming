#include<stdio.h>
int main()
{
    int m;
    printf("enter the marks :\n");
    scanf("%d",&m);
    if (m>=90& m<=100){ 
        printf("grade A");
        
    }
    else if(m>=80&& m<=89){
        printf("grade B");
        
    }
    else if (m>=70&& m<=79){
        printf("grade C");
        
    }
    else if (m>=60&& m<=69){
        printf("grade D");
        
    }
    else if (m<60){
        printf("grade f");
        
    }
    else
    {
        printf("Invalide mark");
        
    }
    return 0;
}
